[mysql]
socket=/var/lib/mysql/mysql.sock
[mysqld]
datadir=/mysql-data
socket=/var/lib/mysql/mysql.sock
performance_schema = off
query_cache_size = 0
innodb_file_per_table
log_bin=mysqld-bin
expire_logs_days=7
symbolic-links=0
sql_mode=NO_ENGINE_SUBSTITUTION,STRICT_TRANS_TABLES
[mysqld_safe]
log-error=/mysql-data/mysqld.log
pid-file=/mysql-data/mysqld.pid
